{
  "description":
    "Replace downloaded map tiles inside OpenStreetMap, with the one queried at a local server.",
  "manifest_version": 2,
  "name": "maps-replacer",
  "version": "1.0",
  "homepage_url": "https://github.com/nathanvogel/DeepMaps",
  "icons": {
    "48": "maps.svg"
  },

  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "downloads",
    "https://www.google.com/*",
    "http://localhost/*",
    "https://localhost/*",
    "https://fiddle.jshell.net/*",
    "https://*.openstreetmap.org/*",
    "https://www.bing.com/*",
    "http://www.blended.study/*",
    "https://storage.googleapis.com/*",
    "https://raw.githubusercontent.com/*",
    "https://mths.be/*"
  ],

  "content_security_policy":
    "default-src 'self'; img-src 'self' data:; script-src 'self' 'unsafe-eval' https://example.com https://fiddle.jshell.net https://www.bing.com; object-src 'self' https://fiddle.jshell.net https://www.bing.com",

  "background": {
    "scripts": [
      "libs/p5.min.js",
      "libs/p5.dom.min.js",
      "libs/ml5.min.js",
      "libs/blob-util.min.js",
      "styleTransfer.js",
      "background.js"
    ]
  },

  "applications": {
    "gecko": {
      "strict_min_version": "57.0a1"
    }
  }
}

{
  "description":
    "Browse online maps as if they were hand-drawn in the 16th century!",
  "manifest_version": 2,
  "name": "maps-replacer",
  "version": "1.1",
  "homepage_url": "https://github.com/nathanvogel/DeepMaps",
  "icons": {
    "48": "maps.svg"
  },

  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "downloads",
    "storage",
    "http://localhost/*",
    "https://localhost/*",
    "https://*.openstreetmap.org/*",
    "https://raw.githubusercontent.com/*"
  ],

  "content_security_policy":
    "default-src 'self'; img-src 'self' data:; script-src 'self' 'unsafe-eval' ; object-src 'self'",

  "background": {
    "scripts": [
      "libs/p5.min.js",
      "libs/p5.dom.min.js",
      "libs/ml5.min.js",
      "libs/blob-util.min.js",
      "styleTransfer.js",
      "background.js"
    ]
  },

  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },

  "applications": {
    "gecko": {
      "strict_min_version": "57.0a1",
      "id": "addon@example.com"
    }
  }
}
